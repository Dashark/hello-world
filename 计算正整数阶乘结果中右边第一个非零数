#include<stdio.h>

int a[]={1,2,4,3};

int b[]={1,1,2,6,4};



int solve(int n){

	if(n<5){

		return b[n];

	}

	int t=n%5,k=n/5;

	return a[k%4]*solve(t)%5*solve(k)%5;

	

}



int main()

{

	

	int num,N;

	scanf("%d\n",&N);

	for(int i=0;i<N;i++)

	{

	

	scanf("%d",&num);

	int ans=solve(num);

	

	if(ans&1){

		ans=(ans+5)%10;

	}

	printf("%d\n",ans);

}

	

	return 0;

}//原理有点难，需要理解
