#include<stdio.h>





int main()

{

	int n;

	int i,s,j,time=0;

	int a[10000]={1};

	scanf("%d",&n);

	s=n;

	if(s>=0)

	{

	for(i=0;s>1;i++)

	{

		if(n%2==0)

		{

			a[i]=0;

		}

		else

		{

			a[i]=1;		

		}

		s=n/2;

		n=n/2;

		}

	if(s==1)

	{

		a[i]=1;

	}

	

	else{

		a[i]=0;

	}

	for(j=i;j>=0;j--)

	{

		if(a[j]==0)

		{

			time=time+1;

		}

	}

	printf("%d",time);

	}

	return 0;

}
